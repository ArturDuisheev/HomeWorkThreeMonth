<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StepTwoRegisterActivity">

    <TextView
        android:id="@+id/textViewMenu"
        android:textSize="18sp"
        android:paddingTop="12dp"
        android:paddingStart="12dp"
        android:textColor="@color/black"
        android:background="@drawable/ic_edit_text_style"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="210dp"
        android:layout_height="46dp"
        android:paddingLeft="12dp"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/textViewMenuTwo"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:paddingTop="12dp"
        android:paddingStart="12dp"
        android:background="@drawable/ic_edit_text_style"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/textViewMenu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="210dp"
        android:layout_height="46dp"
        android:paddingLeft="12dp"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/textViewThree"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:paddingTop="12dp"
        android:paddingStart="12dp"
        android:background="@drawable/ic_edit_text_style"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/textViewMenuTwo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="210dp"
        android:layout_height="46dp"
        android:paddingLeft="12dp"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/textViewFour"
        android:layout_width="210dp"
        android:layout_height="46dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/ic_edit_text_style"
        android:paddingStart="12dp"
        android:paddingLeft="12dp"
        android:paddingTop="12dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewThree"
        tools:ignore="RtlSymmetry" />
</androidx.constraintlayout.widget.ConstraintLayout>